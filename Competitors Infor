#include <stdio.h>
#include <string.h>
struct Ct /* sturucture of competitor. 
{
  char brand[10];
  int rs;         /* Retails Sales, Not include Fleet and rental sales */
  float ms;       /* Market Share */
  int mo,pn;      /* Main outlets Number and Partner No. */
  int spo,spp;    /* Sales per Outlets and per partner */
}
